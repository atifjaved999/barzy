
<div class="row">
    <div class="col-lg-12">
        <h1 class="page-header">User</h1>
        <%#= link_to 'Back', admin_words_path %>
    </div>
    <!-- /.col-lg-12 -->
<div class="row">
  <div class="col-lg-12">
    <div class="panel panel-default">
      <div class="panel-heading">
        User
      </div>
      <!-- /.panel-heading -->
      <div class="panel-body">
        <div class="table-responsive">
          <table class="table">
            <tbody>
              <tr>
                <th>First Name</th>
                <td><%= @admin_user.first_name %></td>
              </tr>
              <tr>
                <th>Last Name</th>
                <td><%= @admin_user.last_name %></td>
              </tr>
              <tr>
                <th>Email</th>
                <td><%= @admin_user.email %></td>
              </tr>
              <tr>
                <th>Address</th>
                <td><%= @admin_user.address %></td>
              </tr>
              <tr>
                <th>Created at</th>
                <td><%= format_date(@admin_user.created_at) rescue '' %></td>
              </tr>
              <tr>
              <td>
                <%= link_to 'Back', admin_users_path, :class=> "btn btn-default" %>
                <%= link_to 'Edit', edit_admin_user_path(@admin_user), :class=> "btn btn-primary" %>
              </td>
              <td>
              </td>
              </tr>

            </tbody>
          </table>
        </div>
        <!-- /.table-responsive -->
      </div>
      <!-- /.panel-body -->
    </div>
    <!-- /.panel -->
  </div>
</div>

<div id= "auth_records"></div>