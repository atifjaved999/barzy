<div class="row">
  <div class="col-lg-12">
    <h1 class="page-header">Editing Product Category</h1>
  </div>
  <!-- /.col-lg-12 -->
</div>

<div class="row">
    <div class="col-lg-12">
        <div class="panel panel-default">
            <div class="panel-heading">
                Product Category Form
            </div>
            <div class="panel-body">
                <div class="row">
                  <%#= form_with(model: @admin_product_category, url: admin_bar_menu_product_category_path(@admin_product_category.menu, @admin_product_category), local: true) do |f| %>
                  <%= nested_form_for ([:admin, @admin_product_category]), url: admin_bar_menu_product_category_path(@admin_product_category), :html => {multipart: true} do |f| %>
                    <%= render "form_body", f:f %>
                  <% end %>
                </div>
                <!-- /.row (nested) -->
            </div>
            <!-- /.panel-body -->
        </div>
        <!-- /.panel -->
    </div>
    <!-- /.col-lg-12 -->
</div>


